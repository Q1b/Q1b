  "compilerOptions": {
    "strictNullChecks": true,
    "types": [
      "@cloudflare/workers-types/2023-07-01"
    ],
    "baseUrl": ".",
    "allowJs": true,
    "paths": {
      "@/*": [
        "src/*"
      ]
